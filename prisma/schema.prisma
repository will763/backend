generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Login {
  id       Int     @id @default(autoincrement())
  email    String  @unique
  name     String?
  password String?

  @@map("login")
}

model Microsoft_Login {
  id       Int     @id @default(autoincrement())
  email    String  @unique
  name     String?

  @@map("microsoft_login")
}
